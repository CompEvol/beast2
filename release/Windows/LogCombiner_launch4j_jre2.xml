<launch4jConfig>
  <headerType>gui</headerType>
  <icon>../common/icons/utility.ico</icon>
  <classPath>
  	<mainClass>beast.app.tools.LogCombiner</mainClass>
  	<cp>./lib/beast.jar</cp>
  </classPath>
  <cmdLine></cmdLine>
  <jre>
    <minVersion>1.8.0</minVersion>
    <initialHeapSize>256</initialHeapSize>
    <maxHeapSize>8096</maxHeapSize>
    <opt>-Duser.language=en</opt>
    <opt>-Dbeast.load.jars=true</opt>
  </jre>    
  <versionInfo>
    <fileDescription>LogCombiner</fileDescription>    
    <copyright>Beast 2 development team 2002-2019</copyright>
    <productName>LogCombiner</productName>
    <internalName>LogCombiner</internalName>
    <originalFilename>LogCombiner.exe</originalFilename>    
  </versionInfo>
</launch4jConfig>
