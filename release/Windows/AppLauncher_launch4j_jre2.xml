<launch4jConfig>
  <headerType>gui</headerType>
  <icon>../common/icons/utility.ico</icon>
  <classPath>
  	<mainClass>beast.app.tools.AppLauncher</mainClass>
  	<cp>./lib/beast.jar</cp>
  </classPath>
  <cmdLine></cmdLine>
  <jre>
    <minVersion>1.6.0</minVersion>
    <initialHeapSize>256</initialHeapSize>
    <maxHeapSize>8096</maxHeapSize>
    <opt>-Duser.language=en</opt>
    <opt>-Dbeast.load.jars=true</opt>
    <path>jre</path>
  </jre>    
  <versionInfo>
    <fileDescription>AppLauncher</fileDescription>
    <copyright>Beast 2 development team 2002-2019</copyright>
    <productName>AppLauncher</productName>
    <internalName>AppLauncher</internalName>
    <originalFilename>AppLauncher.exe</originalFilename>
  </versionInfo>
</launch4jConfig>
